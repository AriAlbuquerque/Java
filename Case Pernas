import java.util.Scanner;
import java.math.*;
import java.math.MathContext;

public class ProjetoCase {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.println("Digite quantas pernas:");
        int pernas = sc.nextInt();
        String tipo;

        switch (pernas) {
            case 1:
                tipo = "Saci";
                break;
            case 2:
                tipo = "Bipede";
                break;
            case 3:
                tipo = "Não existe :p";
                break;
            case 4:
                tipo = "Quadrupede";
                break;
            case 5:
                tipo = "Nao existe dãaaa...";
                break;
            case 6:
                tipo = "Aranha";
                break;
            default:
                tipo = "ET certeza!!!! :0";
        }
        System.out.println(tipo);

        {


        }
    }
}
