import java.math.*;
import java.util.Scanner;


public class Tabuada {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        int x;
        System.out.println("Digite a tabuada que deseja: ");
         int tab = sc.nextInt();

        for (x = 0; x <= 10; x++) {
            System.out.println( + tab + " x " + x + " = " + tab*x);

        }

    }
}
